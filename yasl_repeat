#!yasl_linux
{
	(# 0 ==) ? (0 exit) : (
		1:=:2
		2:%@(0:=:3
			3:print
			1:2
			2:-:2
			)
	0:!
	1:1
	1:+:1
	"\n"
	print
	)
}repeat
!!
(# 0 ==) ? ("please provide two parameters"print 0 exit)
0:=:1
(0 <=) ? ("the first argument start_num must be a positive number\n"print 0 exit)
(# 0 ==) ? (0 exit) : (# #repeat)
